<div ng-if="data.length>0" class="row mt-5">
    <div class="col pt-3" ng-repeat="x in data">
        <img src="../assets/img/icon/{{x.icon}}" class="service-img" alt="{{x.icon}}" />
        <h3 class="mt-2" style="font-size:1.1rem">
            {{x.title | translate}}
        </h3>
    </div>
</div>
